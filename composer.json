{
  "name": "rolice/laravel-db-switch",
  "description": "Simple and light DB switcher for Laravel.",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.0",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "l.gardev@intellisys.org"
    }
  ],
  "keywords": ["php", "laravel", "db", "database", "switch", "runtime"],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "^5.2",
    "illuminate/database": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "Rolice\\LaravelDbSwitch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rolice\\LaravelDbSwitch\\Test\\": "tests/"
    }
  }
}